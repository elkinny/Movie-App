{
  "env": {
      "browser": true,
      "es6": true,
      "jest": true
  },
  "extends": [ "eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["import", "jest", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": [
          "error",
          { "allow": ["error", "log"] }
      ]
  }
}